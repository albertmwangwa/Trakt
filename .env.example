# Environment variables for Trakt configuration
# Copy this file to .env and update with your actual values

# Camera Configuration
CAMERA_HOST=192.168.1.100
CAMERA_PORT=80
CAMERA_USERNAME=admin
CAMERA_PASSWORD=password

# OCR Configuration
OCR_ENGINE=easyocr  # Options: tesseract, easyocr
OCR_CONFIDENCE_THRESHOLD=0.5

# TensorFlow Configuration
TF_USE_GPU=false
TF_GPU_MEMORY_LIMIT=2048

# Output Configuration
OUTPUT_SAVE_RESULTS=true
OUTPUT_SAVE_FRAMES=true
OUTPUT_LOG_LEVEL=INFO

# Application Configuration
APP_SHOW_PREVIEW=false
APP_FPS_LIMIT=5
